{
  "root": "services/todos",
  "projectType": "application",
  "sourceRoot": "services/todos/src",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "services/todos",
        "color": true,
        "command": "sam build"
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "services/todos",
        "color": true,
        "command": "sam local start-api -d 8065"
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "services/todos",
        "color": true,
        "command": "sam deploy"
      },
      "dependsOn": [
        {
          "target": "deploy",
          "projects": "dependencies"
        }
      ]
    },
    "remove": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "services/todos",
        "color": true,
        "command": "sam delete"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["services/todos/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/services/todos"],
      "options": {
        "jestConfig": "services/todos/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["service"],
  "implicitDependencies": ["core"]
}
